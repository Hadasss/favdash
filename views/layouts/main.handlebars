<body>
  <nav class="navbar has-background-info-light" role="navigation" aria-label="main-navigation">
    {{#if loggedIn}}
    <div class="navbar-brand">
      <a href="/dashboard" class="navbar-item title has-text-info-dark ml-5">Dashboard</a>
    </div>
    <div class="navbar-end">
      <div class="navbar-item">
        <div class="buttons">
          <a class="button is-info is-rounded mr-5">
            <strong id="logout">Logout</strong>
          </a>
        </div>
      </div>
    </div>
    {{else}}
    <div class="navbar-brand has-background-info-light">
      <p class="navbar-item title is-family-secondary ml-5 has-text-info-dark">Fav-Dash</p>
    </div>
    <div class="navbar-end">
      <div class="navbar-item">
        <div class="buttons">
          <a class="button is-info is-rounded mr-5" href="/login">
            <strong>Login</strong>
          </a>
        </div>
      </div>
    </div>

    {{/if}}
  </nav>
